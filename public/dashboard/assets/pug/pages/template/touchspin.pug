- var page_Title = 'Touchspin';
- var touchspin_2           = true;
- var theme_customizer      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var scrollbar             = true;

doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-sm-6
                  h3
                    | Touchspin
                .col-sm-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Form Widgets
                    li.breadcrumb-item.active Touchspin
          // Container-fluid starts
          .container-fluid
            // Custom TouchSpin Spinners start
            .bootstrap-touchspin
              .row
                .col-xl-6
                  .card
                    .card-header
                      h5 Default Touchspin
                      p.f-m-light.mt-1 
                        | Use 
                        code decrement-touchspin 
                        | and 
                        code increment-touchspin 
                        | class for default style touchspin.
                    .card-body.common-flex
                      .touchspin-wrapper 
                        button.decrement-touchspin.btn-touchspin.touchspin-primary
                          i.fa-solid.fa-minus 
                        input.input-touchspin.spin-outline-primary(type='number' value='0')
                        button.increment-touchspin.btn-touchspin.touchspin-primary
                          i.fa-solid.fa-plus  
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.touchspin-secondary
                          i.fa-solid.fa-minus  
                        input.input-touchspin.spin-outline-secondary(type='number' value='2')
                        button.increment-touchspin.btn-touchspin.touchspin-secondary
                          i.fa-solid.fa-plus   
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.touchspin-success
                          i.fa-solid.fa-minus  
                        input.input-touchspin.spin-outline-success(type='number' value='1')
                        button.increment-touchspin.btn-touchspin.touchspin-success
                          i.fa-solid.fa-plus   
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.touchspin-danger
                          i.fa-solid.fa-minus  
                        input.input-touchspin.spin-outline-danger(type='number' value='8')
                        button.increment-touchspin.btn-touchspin.touchspin-danger
                          i.fa-solid.fa-plus   
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.touchspin-warning
                          i.fa-solid.fa-minus  
                        input.input-touchspin.spin-outline-warning(type='number' value='3')
                        button.increment-touchspin.btn-touchspin.touchspin-warning
                          i.fa-solid.fa-plus   
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.touchspin-info
                          i.fa-solid.fa-minus
                        input.input-touchspin.spin-outline-info(type='number' value='9')
                        button.increment-touchspin.btn-touchspin.touchspin-info
                          i.fa-solid.fa-plus   
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.touchspin-dark
                          i.fa-solid.fa-minus     
                        input.input-touchspin.spin-outline-dark(type='number' value='4')
                        button.increment-touchspin.btn-touchspin.touchspin-dark
                          i.fa-solid.fa-plus   
                .col-xl-6
                  .card
                    .card-header
                      h5 Outlined Touchspin
                      p.f-m-light.mt-1 
                        | Use 
                        code decrement-touchspin 
                        | and 
                        code increment-touchspin 
