                        button.increment-touchspin.btn-touchspin.spin-border-secondary
                          i.fa-solid.fa-plus   
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.spin-border-success
                          i.fa-solid.fa-minus  
                        input.input-touchspin.spin-outline-success(type='number' value='1')
                        button.increment-touchspin.btn-touchspin.spin-border-success
                          i.fa-solid.fa-plus   
                      .touchspin-wrapper
                        button.decrement-touchspin.btn-touchspin.spin-border-danger
                          i.fa-solid.fa-minus  
                        input.input-touchspin.spin-outline-danger(type='number' value='8')
