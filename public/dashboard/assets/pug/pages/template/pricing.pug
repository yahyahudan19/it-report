- var page_Title = 'Pricing';
- var theme_customizer      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var scrollbar             = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-sm-6
                  h3
                    | Pricing
                .col-sm-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Pages
                    li.breadcrumb-item.active Pricing
          // Container-fluid starts
          .container-fluid
            .row
              .col-sm-12
                .card
                  .card-header 
                    h5 Become a member
                  .card-body  
                    .row.g-3.pricing-block
                      .col-lg-3.col-sm-6
                        .pricingtable
                          .pricingtable-header
                            h3.title Standard
                          .price-value
                            span.currency $
                            span.amount 10
                            span.duration /mo
                          ul.pricing-content
                            li 50GB Disk Space
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                      .col-lg-3.col-sm-6
                        .pricingtable
                          .pricingtable-header
                            h3.title Premium
                          .price-value
                            span.currency $
                            span.amount 20
                            span.duration /mo
                          ul.pricing-content
                            li 10% on all product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                      .col-lg-3.col-sm-6
                        .pricingtable
                          .pricingtable-header
                            h3.title Business
                          .price-value
                            span.currency $
                            span.amount 50
                            span.duration /mo
                          ul.pricing-content
                            li Upload 50 product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                      .col-lg-3.col-sm-6
                        .pricingtable
                          .pricingtable-header
                            h3.title Author pack
                          .price-value
                            span.currency $
                            span.amount 75
                            span.duration /mo
                          ul.pricing-content
                            li Upload 50 product
                            li 50 Email Accounts
                            li Maintenance
                            li 15 Subdomains
                          .pricingtable-signup
                            a.btn.btn-primary.btn-lg(href='#') Sign Up
                .card
                  .card-header 
                    h5 Simple Pricing Card
                  .card-body.pricing-content
                    .row.g-sm-4.g-3
                      .col-xxl-3.col-sm-6.box-col-6
                        .card.text-center.pricing-simple.h-100
                          .card-body
                            h4 Standard
                            h5 $15
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | No setup costs
                              li 
                                i.fa-solid.fa-circle-check
                                | Hotline Support for 24/7
                              li 
                                i.fa-solid.fa-circle-check
                                | Priority Support
                              li 
                                i.fa-solid.fa-circle-check
                                | Key Components
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
                      .col-xxl-3.col-sm-6.box-col-6
                        .card.text-center.pricing-simple.h-100
                          .card-body
                            h4 Business
                            h5 $25
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | 40+ Demos
                              li 
                                i.fa-solid.fa-circle-check
                                | Customer Supports
                              li 
                                i.fa-solid.fa-circle-check
                                | Email Supports
                              li 
                                i.fa-solid.fa-circle-check
                                | Basic Analytics
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
                      .col-xxl-3.col-sm-6.box-col-6
                        .card.text-center.pricing-simple.h-100
                          .card-body
                            h4 Premium
                            h5 $35
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | Priority Email Supports
                              li 
                                i.fa-solid.fa-circle-check
                                | Customizable Dashboards
                              li 
                                i.fa-solid.fa-circle-check
                                | Advanced Analytics
                              li 
                                i.fa-solid.fa-circle-check
                                | All Basic Features
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
                      .col-xxl-3.col-sm-6.box-col-6
                        .card.text-center.pricing-simple.h-100
                          .card-body
                            h4 Extra
                            h5 $45
                            ul.pricing-content
                              li 
                                i.fa-solid.fa-circle-check
                                | Customized Onboarding and Training
                              li 
                                i.fa-solid.fa-circle-check
                                | Hotline Support for 24/7
                              li 
                                i.fa-solid.fa-circle-check
                                | Access to all templates
                              li 
                                i.fa-solid.fa-circle-check
                                | Priority email support
                          a.btn.btn-lg.btn-primary.btn-block(href='#') Purchase
        include ../../components/footer
    include ../../components/footer-files
