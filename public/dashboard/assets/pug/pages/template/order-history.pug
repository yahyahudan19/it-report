                .card
                  .card-body
                    .row.g-3.custom-input
                      //- From date 
                      .col-xl.col-md-6 
                        label.form-label(for='datetime-local') From: 
                        .input-group.flatpicker-calender
                          input#datetime-local.form-control(placeholder='dd/mm/yyyy')
                      //- To date 
                      .col-xl.col-md-6 
                        label.form-label(for='datetime-local3') To: 
                        .input-group.flatpicker-calender
                          input#datetime-local3.form-control(placeholder='dd/mm/yyyy')
                      //- Payment status
                      .col-xl.col-md-6
                        label.form-label Payment Status
                        select.form-select
                          option(selected='') Completed
                          option(value='1') Pending
                          option(value='2') Failed
                      //- Payment Methods
                      .col-xl.col-md-6
                        label.form-label Payment Methods
                        select.form-select
                          option(selected='') Paypal
                          option(value='1') COD
                          option(value='2') Bank transfer
                          option(value='3') Credit card
                      .col.d-flex.justify-content-start.align-items-center.m-t-40
                        a.btn.btn-primary.f-w-500(href="#!") Submit
