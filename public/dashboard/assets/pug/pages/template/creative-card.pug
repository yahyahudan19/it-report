- var page_Title = 'Creative Card';
- var theme_customizer      = true;
- var sidebar               = true;
- var slick                 = true;
- var header_slider         = true;
- var animate               = true;
- var scrollbar             = true;
- var height_equal          = true;
doctype html
html(lang='en')
  include ../../components/header-files
  body
    include ../../components/loader
    include ../../components/taptop
    // page-wrapper Start
    #pageWrapper.page-wrapper.compact-wrapper
      include ../../components/header
      // Page Body Start
      .page-body-wrapper
        include ../../components/sidebar
        .page-body
          .container-fluid
            .page-title
              .row
                .col-sm-6
                  h3
                    | Creative Card
                .col-sm-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item  Bonus Ui
                    li.breadcrumb-item.active Creative Card
          // Container-fluid starts
          .container-fluid
            .row.effective-card.height-equal
              .col-xxl-3.col-xl-6.col-12.box-col-4
                .card.common-hover
                  .card-body
                    a.effect-card(href='#!')
                      div.common-box1.common-align
                        i(data-feather='plus-square')
