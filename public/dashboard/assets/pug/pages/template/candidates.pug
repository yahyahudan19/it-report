            .page-title
              .row
                .col-sm-6
                  h3
                    | Candidates
                .col-sm-6
                  ol.breadcrumb
                    li.breadcrumb-item
                      a(href='index.html')                                        
                        svg.stroke-icon
                          use(href='../assets/svg/icon-sprite.svg#stroke-home')
                    li.breadcrumb-item Jobs
                    li.breadcrumb-item.active Candidates
          // Container-fluid starts
